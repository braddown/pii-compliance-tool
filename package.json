{
  "name": "pii-compliance-tool",
  "version": "0.1.0",
  "description": "GDPR/CCPA/APP compliance toolkit for Next.js applications - PII management, consent tracking, and audit logging",
  "author": "Your Name",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    },
    "./migrations/*": "./migrations/*"
  },
  "files": [
    "dist",
    "migrations",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "date-fns": "^3.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "gdpr",
    "compliance",
    "ccpa",
    "app",
    "privacy",
    "pii",
    "consent",
    "audit-log",
    "data-subject-request",
    "nextjs",
    "react",
    "supabase"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/pii-compliance-tool"
  },
  "bugs": {
    "url": "https://github.com/your-org/pii-compliance-tool/issues"
  },
  "homepage": "https://github.com/your-org/pii-compliance-tool#readme"
}
